@model DemoWorkBounty.Models.Register
@{
    ViewBag.Title = "Signup";
    Layout = "~/Views/_LayoutPage2.cshtml";
}
<script src="~/assets/js/jquery.2.1.1.min.js"></script>
<script src="~/assets/js/bootstrap-datepicker.min.js"></script>
<script type="text/javascript" src="jquery-1.11.3-jquery.min.js"></script>
<script type="text/javascript" src="validation.min.js"></script>
<link rel="stylesheet" href="~/assets/css/datepicker.min.css">
<link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
<script src="~/assets/js/datepicker.js"></script>
<script src="~/assets/js/jquery_mobile_custom_start.js"></script>
<script src="~/assets/js/jq_script_src.js"></script>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="main-content">
        <div class="row">
            <div class="col-sm-10 col-sm-offset-1">
                <div class="login-container">
                    <div class="center">
                    </div>

                    <div class="space-6"></div>

                    <div class="position-relative">
                        <div id="signup-box" class="signup-box widget-box no-border">
                            <div class="widget-body">
                                <div class="widget-main">
                                    <h4 class="header green lighter bigger">
                                        <i class="ace-icon fa fa-users blue"></i>
                                        New User Registration
                                    </h4>

                                    <div class="space-6"></div>
                                    <p> Enter your details to begin: </p>


                                    <form id="form1">
                                        <fieldset>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">

                                                    @Html.TextBoxFor(a => a.EmailID, new { @class = "form-control", @placeholder = "E mail" })


                                                </span>
                                                @Html.ValidationMessageFor(a => a.EmailID)
                                            </label>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">

                                                    @Html.TextBoxFor(a => a.Phone, new { @class = "form-control", @placeholder = "Number" })

                                                </span>
                                                @Html.ValidationMessageFor(a => a.Phone)
                                            </label>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    @Html.TextBoxFor(a => a.Firstname, new { @class = "form-control", @placeholder = "First Name" })
                                                </span>
                                                @Html.ValidationMessageFor(a => a.Firstname)
                                            </label>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    @Html.TextBoxFor(a => a.Lastname, new { @class = "form-control", @placeholder = "Last Name" })

                                                </span>
                                                @Html.ValidationMessageFor(a => a.Lastname)
                                            </label>
                                            <div class="block clearfix">
                                                <div class="hero-unit">
                                                    @Html.TextBoxFor(a => a.DateofBirth, new { @class = "form-control", @placeholder = "Date of Birth", @id = "DateSelect", @name = "DateofBirth" })

                                                </div>
                                            </div>

                                            <label class="block clearfix">
                                                <span class="block input-icon input-icon-right">
                                                    @Html.PasswordFor(a => a.Password, new { @class = "form-control", @placeholder = "Password" })
                                                </span>
                                                @Html.ValidationMessageFor(a => a.Password)
                                            </label>


                                            <div class="space-6"></div>

                                            <div class="clearfix">

                                                <div>
                                                    @*@Html.ActionLink("I want to register", "Signup", new{@class="user-signup-link" })*@

                                                    <a href="~/Home/Login" class="user-signup-link">
                                                        <i class="ace-icon fa fa-arrow-left"></i> Back To login

                                                    </a>

                                                    <button type="submit" class="width-35 pull-right btn btn-sm btn-primary">
                                                        <i class="ace-icon fa fa-key"></i>
                                                        <span class="bigger-110">Signup</span>
                                                    </button>
                                                </div>


                                            </div>
                                        </fieldset>
                                    </form>
                                </div>

                            </div><!-- /.widget-body -->
                        </div>

                    </div><!-- /.position-relative -->


                </div>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.main-content -->
    <!-- basic scripts -->
    <!--[if !IE]> -->
    <script src="assets/js/jquery.2.1.1.min.js"></script>
   



    
}

