@model List<DemoWorkBounty.Models.ViewMyTeam>
@{
    ViewBag.Title = "myteam";
    Layout = "~/Views/_LayoutPage1.cshtml";
}


<div class="widget-box transparent" id="recent-box">
   
  
     <div class="widget-header">
        <div class="col-xs-10">
            <h4 class="widget-title lighter smaller">
               Team |
                @Html.ActionLink("Add Team", "addteam", "Team")
            </h4>
        </div>
  
        <div class="row">
            
            <div class="col-xs-4">
            
                <div class=".col-xs-4.col-sm-2 no-padding-right">
                
          
                <table id="simple-table" class="table table-striped table-bordered table-hover">
                   

                    <thead>
                        <tr>

                            <th>Member</th>
                            <th>Email</th>
                            <th>Contact No</th>
                        </tr>
                    </thead>
                    @foreach (var item in Model)
                        {
                    <tbody>
                            <tr><td>@Html.DisplayFor(s=>item.FirstName)</td>
                                <td>@Html.DisplayFor(s=>item.Email)</td>
                                <td>@Html.DisplayFor(s=>item.PhoneNumber)</td>
                        
                        
                            </tr>
                    </tbody>
                    }
                </table>
            
            </div>

            
            
        </div>
               

        <div class="row">
            <div class="col-xs-4"></div>
            <ul class="pagination">
                <li class="disabled">
                    <a href="#">
                        <i class="ace-icon fa fa-angle-double-left"></i>
                    </a>
                </li>
                <li class="active">
                    <a href="#">1</a>
                </li>
                <li>
                    <a href="#">2</a>
                </li>
                <li>
                    <a href="#">3</a>
                </li>
                <li>
                    <a href="#">4</a>
                </li>
                <li>
                    <a href="#">5</a>
                </li>
                <li>
                    <a href="#">
                        <i class="ace-icon fa fa-angle-double-right"></i>
                    </a>
                </li>
            </ul>

        </div>
    </div>
    }
</div>
